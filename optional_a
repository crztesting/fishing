while task.wait(1) do

    local args = {
        [1] = "Platinum Fishing Rod"
    }
    game:GetService("ReplicatedStorage"):WaitForChild("Network"):WaitForChild("FishingMerchant_PurchaseRod"):InvokeServer(unpack(args))
end

